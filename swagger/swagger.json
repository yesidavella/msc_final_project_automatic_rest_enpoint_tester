  "host": "localhost:8484",
  "basePath": "/v2",
  "schemes": [
    "http"
  ],
  "paths": {
    "/path/{id}": {
      "get": {
        "tags": [
          "silver bullet"
        ],
        "summary": "Find something by ID",
        "description": "Returns",
        "operationId": "getById",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID to return",
            "required": true,
            "type": "string"
          },
          {
            "name": "limit",
            "in": "query",
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "required": false,
            "description": "The number of items allowed to return."
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          },
          "400": {
            "description": "Invalid ID supplied"
          }
        }
      }
    }
  }
}